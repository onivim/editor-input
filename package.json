  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "^3.5.2",
    "refmterr": "*",
    "@opam/ppx_deriving": ">=4.4",
    "ocaml": "^4.7.0"
  },
  "devDependencies": {
    "ocaml": "^4.8.0",
    "@opam/ocaml-lsp-server": "github:ocaml/ocaml-lsp:ocaml-lsp-server.opam#04733ed"
  }
}
